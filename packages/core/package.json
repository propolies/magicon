{
	"name": "@magicon/core",
	"version": "0.0.10",
	"description": "Fast, typesafe Icon wrapper for svelte",
	"keywords": [
		"icons",
		"heroicons",
		"svelte"
	],
	"author": {
		"name": "propolies",
		"url": "https://github.com/propolies"
	},
  "repository": {
    "type": "git",
    "url": "git+https://github.com/propolies/magicon.git",
    "directory": "package"
  },
	"homepage": "https://propolies.github.io/magicon/",
	"scripts": {
		"dev": "rimraf dist && svelte-package --watch",
		"build": "rimraf dist && vite build && svelte-kit sync && svelte-package && publint",
		"test": "cd tests && pnpm test"
	},
	"exports": {
		".": {
			"types": "./dist/index.d.ts",
			"svelte": "./dist/index.js"
		},
		"./plugins": {
			"types": "./dist/plugins/index.d.ts",
			"import": "./dist/plugins/index.js"
		}
	},
	"files": [
		"dist",
		"!dist/**/*.test.*",
		"!dist/**/*.spec.*"
	],
	"peerDependencies": {
		"svelte": "^4.0.0 || ^5.0.0 || ^5.0.0-next"
	},
	"devDependencies": {
		"@sveltejs/adapter-auto": "^3.0.0",
		"@sveltejs/kit": "^2.0.0",
		"@sveltejs/package": "^2.0.0",
		"@sveltejs/vite-plugin-svelte": "^3.0.0",
		"magic-string": "^0.30.10",
		"publint": "^0.1.9",
		"svelte-check": "^3.6.0",
		"typescript": "^5.0.0",
		"vite": "^5.0.11"
	},
	"svelte": "./dist/index.js",
	"types": "./dist/index.d.ts",
	"type": "module",
  "main": "./dist/index.js"
}
